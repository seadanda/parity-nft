{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "framework": "nextjs",
  "functions": {
    "src/app/api/mint/route.ts": {
      "maxDuration": 300
    },
    "src/app/api/**/*.ts": {
      "maxDuration": 60
    }
  },
  "regions": ["iad1"],
  "env": {
    "NODE_OPTIONS": "--max-old-space-size=4096"
  }
}
